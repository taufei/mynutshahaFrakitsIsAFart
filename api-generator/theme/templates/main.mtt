<!DOCTYPE html>
<html lang="en">

<!-- use theme color or fallback -->
::set themeColor="0xFAFAFA"::
::if (api.isDefined("themeColor"))::::set themeColor=api.getValue("themeColor")::::end::

<!--use textcolor from settings, otherwise create a contrasting color to theme color-->
::set textColor="0xFFFFFF"::
::if (api.isDefined("textColor"))::::set textColor=api.getValue("textColor")::
::elseif (api.std.parseInt(themeColor) > 7829367)::::set textColor="0x000000"::::end::

::use "header.mtt"::::end::
<body>
<div class="header">
	::use "topbar.mtt"::::end::
</div>
<div class="container">
	<div class="sidebar-wrapper">
		<input type="checkbox" class="sidebar-button" />
		<span class="sidebar-button-front">
			<svg viewBox="0 0 100 80" width="40" height="40">
				<rect width="100" height="20" rx="10"></rect>
				<rect y="30" width="100" height="20" rx="10"></rect>
				<rect y="60" width="100" height="20" rx="10"></rect>
			</svg>
			<svg width="800px" height="800px" viewBox="0 0 1024 1024" xmlns="http://www.w3.org/2000/svg"><path stroke="#000000" d="M195.2 195.2a64 64 0 0 1 90.496 0L512 421.504 738.304 195.2a64 64 0 0 1 90.496 90.496L602.496 512 828.8 738.304a64 64 0 0 1-90.496 90.496L512 602.496 285.696 828.8a64 64 0 0 1-90.496-90.496L421.504 512 195.2 285.696a64 64 0 0 1 0-90.496z"/></svg>
		</span>

		<div class="sidebar">
			<ul class="sidebar-unordered-list">
				<li><form class="form-search" id="searchForm">
					<div class="input-prepend input-block-level">
						<input id="search" type="text" placeholder="Filter (Ctrl+P)" autocomplete="off"/>
					</div>
					</form></li>
				<li><a href="::api.config.rootPath::">
					<i class="fa fa-home"></i>
					Top-level
				</a></li>
			</ul>
			<span id="nav">
				$$comment(::" Automatically populated by index.js "::)
			</span>
		</div>
	</div>
	<div class="content">::raw __content__::</div>
</div>

::use "footer.mtt"::::end::


<script src="::api.config.rootPath::highlighter.js"></script>
<link href="::api.config.rootPath::highlighter.css" rel="stylesheet" />

</body>
</html>